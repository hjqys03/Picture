{
    "log": {
        "level": "error",
        "timestamp": true
    },
    "dns": {
        "servers": [
            {
                "tag": "cloudflare",
                "type": "tls",
                "server": "8.8.8.8",
                "detour": "proxy"
            },
            {
                "tag": "dnspod",
                "type": "tcp",
                "server": "223.5.5.5",
                "detour": "国内代理"
            },
            {
                "tag": "alidns",
                "type": "tcp",
                "server": "223.5.5.5"
            },
            {
                "tag": "dns_fakeip",
                "type": "fakeip",
                "inet4_range": "198.18.0.0/15",
                "inet6_range": "fc00::/18"
            }
        ],
        "rules": [
            {
                "rule_set": [
                    "domain-category-ads-all"
                ],
                "action": "predefined",
                "rcode": "NOERROR"
            },
            {
                "query_type": [
                    "A",
                    "AAAA"
                ],
                "action": "route",
                "server": "dns_fakeip",
                "rewrite_ttl": 1
            },
            {
                "rule_set": [
                    "domain-cn"
                ],
                "server": "dnspod"
            }
        ],
        "final": "cloudflare",
        "independent_cache": true
    },
    "inbounds": [
        {
            "tag": "tun",
            "type": "tun",
            "interface_name": "sing-box",
            "address": [
                "172.18.0.1/16",
                "fdfe:dcba:9876::1/126"
            ],
            "strict_route": true,
            "auto_route": true,
            "stack": "system"
        }
    ],
    "outbounds": [
        {
            "type": "direct",
            "tag": "direct",
            "domain_resolver": "dnspod"
        },
        {
            "type": "selector",
            "tag": "国内代理",
            "outbounds": [
                "快游戏",
                "direct",
                "百度"
            ]
        },
        {
            "type": "selector",
            "tag": "百度",
            "providers": [
                "百度直连"
            ]
        },
        {
            "type": "selector",
            "tag": "快游戏",
            "providers": [
                "XiReiki Remote"
            ]
        },
        {
            "type": "selector",
            "tag": "proxy",
            "outbounds": [
                "Remote"
            ]
        },
        {
            "type": "selector",
            "tag": "Remote",
            "providers": [
                "XiReiki Remote"
            ]
        }
    ],
    "route": {
        "rules": [
            {
                "action": "sniff"
            },
            {
                "type": "logical",
                "mode": "or",
                "rules": [
                    {
                        "port": 53
                    },
                    {
                        "protocol": "dns"
                    }
                ],
                "action": "hijack-dns"
            },
            {
                "rule_set": "domain-category-ads-all",
                "action": "reject"
            },
            {
                "clash_mode": "direct",
                "outbound": "direct"
            },
            {
                "clash_mode": "global",
                "outbound": "proxy"
            },
            {
                "ip_is_private": true,
                "action": "reject"
            },
            {
                "type": "logical",
                "mode": "or",
                "rules": [
                    {
                        "port": 853
                    },
                    {
                        "network": "udp",
                        "port": 443
                    },
                    {
                        "protocol": [
                            "stun",
                            "quic"
                        ]
                    }
                ],
                "action": "reject"
            },
            {
                "rule_set": "geosite-geolocation-!cn",
                "outbound": "proxy"
            },
            {
                "rule_set": [
                    "domain-cn"
                ],
                "outbound": "国内代理"
            },
            {
                "action": "resolve"
            },
            {
                "rule_set": [
                    "ip-cn"
                ],
                "outbound": "国内代理"
            }
        ],
        "rule_set": [
            {
                "tag": "ip-cn",
                "type": "remote",
                "update_interval": "24h",
                "download_detour": "proxy",
                "url": "tcp://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo/geoip/cn.srs",
                "format": "binary"
            },
            {
                "tag": "domain-category-ads-all",
                "type": "remote",
                "update_interval": "24h",
                "download_detour": "proxy",
                "url": "tcp://github.com/Yuu518/sing-box-rules/raw/rule_set/rule_set_site/category-ads-all.srs",
                "format": "binary"
            },
            {
                "tag": "geosite-geolocation-!cn",
                "type": "remote",
                "format": "binary",
                "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo/geosite/geolocation-!cn.srs",
                "download_detour": "proxy"
            },
            {
                "tag": "domain-cn",
                "type": "remote",
                "update_interval": "24h",
                "download_detour": "proxy",
                "url": "tcp://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo/geosite/cn.srs",
                "format": "binary"
            }
        ],
        "default_domain_resolver": {
            "server": "alidns"
        },
        "auto_detect_interface": true,
        "final": "proxy"
    },
    "experimental": {
        "clash_api": {
            "external_controller": "127.0.0.1:9909",
            "external_ui": "dashboard",
            "default_mode": "rule"
        },
        "cache_file": {
            "enabled": true,
            "path": "cache.db",
            "store_fakeip": true
        },
        "urltest_unified_delay": true
    },
    "outbound_providers": [
        {
            "type": "local",
            "tag": "百度直连",
            "path": "百度直连.yaml",
            "outbound_override": {
                "tcp_fast_open": false
            }
        },
        {
            "type": "remote",
            "tag": "XiReiki Remote",
            "download_url": "",
            "path": "Remote.json",
            "outbound_override": {
                "tcp_fast_open": false
            }
        }
    ]
}