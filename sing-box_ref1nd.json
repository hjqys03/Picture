{
  "inbounds": [
    {
      "type": "mixed",
      "listen": "0.0.0.0",
      "listen_port": 9890
    },
    {
      "type": "tproxy",
      "listen": "0.0.0.0",
      "listen_port": 1536
    }
  ],
  "dns": {
    "servers": [
      {
        "type": "https",
        "tag": "Resolve-DNS",
        "server": "223.5.5.5"
      },
      {
        "type": "https",
        "tag": "Direct-DNS",
        "domain_resolver": "Resolve-DNS",
        "server": "dns.alidns.com"        
      },
      {
        "type": "https",
        "tag": "Proxy-DNS",
        "detour": "Main",
        "server": "8.8.8.8"
      },
      {
        "type": "fakeip",
        "tag": "fakeip",
        "inet4_range": "198.18.0.0/16",
        "inet6_range": "fc00::/18"
      }
    ],
    "rules": [
      {
        "clash_mode": "direct",
        "server": "Direct-DNS"
      },
      {
        "clash_mode": "global",
        "server": "Proxy-DNS"
      },      
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "clash_mode": "PassAD",
            "invert": true
          },
          {
            "rule_set": "FuckAD"
          }
        ],
        "action": "reject"
      },
      {
        "domain_suffix": [
          "googleapis.cn",
          "xn--ngstr-Ira8j.com"
        ],
        "server": "Proxy-DNS"
      },      
      {
        "rule_set": "FCM",
        "action": "route",
        "server": "Proxy-DNS"
      },
      {
        "rule_set": "!CN",
        "query_type": [
          "A",
          "AAAA"
        ],
        "server": "fakeip",
        "rewrite_ttl": 1
      },
      {
        "rule_set": "CN",
        "server": "Direct-DNS"
      }       
    ],
    "final": "Direct-DNS",
    "reverse_mapping": true,
    "cache_capacity": 2048
  },
  "route": {
    "final": "Direct",
    "auto_detect_interface": true,
    "find_process": true,
    "default_domain_resolver": "Direct-DNS",
    "rules": [
      {
        "network": "icmp",
        "outbound": "Direct"
      },
      {
        "action": "sniff",
        "sniffer": [
          "http",
          "tls",
          "quic"
        ]
      },
      {
        "port": 53,
        "action": "hijack-dns"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },
      {
        "ip_is_private": true,
        "outbound": "Direct"
      },
      {
        "clash_mode": "direct",
        "outbound": "Direct"
      },
      {
        "clash_mode": "global",
        "outbound": "Global"
      },
      {
        "domain_suffix": [
          "googleapis.cn",
          "xn--ngstr-Ira8j.com"
        ],
        "outbound": "Main"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "network": "udp",
            "port": 443
          },
          {
            "protocol": "stun"
          }
        ],
        "action": "reject"
      },
      {
        "rule_set": "Telegram_IP",
        "outbound": "Telegram"
      },
      {
        "rule_set": "FCM",
        "outbound": "Direct"
      },
      {
        "rule_set": "Spotify",
        "outbound": "Spotify"
      },
      {
        "rule_set": "AI",
        "outbound": "AI"
      },
      {
        "rule_set": "GitHub",
        "outbound": "GitHub"
      },
      {
        "rule_set": "!CN",
        "outbound": "Main"
      },
      {
        "rule_set": "CN_IP",
        "outbound": "Direct"
      }      
    ],
    "rule_set": [
      {
        "type": "remote",
        "tag": "FuckAD",
        "url": "https://ghfast.top/raw.githubusercontent.com/Chocolate4U/Iran-sing-box-rules/rule-set/geosite-category-ads-all.srs",
